{
    "name": "pdaleramirez/asymmetric-encryption",
    "description": "A laravel package that encrypts your data with private public key pairs using asymmetric encryption.",
    "keywords": [
        "laravel",
        "encrypt",
        "encryption",
        "asymmetric encryption"
    ],
    "homepage": "https://github.com/pdaleramirez/asymmetric-encryption",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "autoload": {
        "psr-4": {
            "pdaleramirez\\asymmetric\\encryption\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "pdaleramirez\\asymmetric\\encryption\\AsymmetricEncryptionProvider"
            ],
            "aliases": {
                "AsymmetricEncryption": "pdaleramirez\\asymmetric\\encryption\\AsymmetricEncryptionFacade"
            }
        }
    }
}
